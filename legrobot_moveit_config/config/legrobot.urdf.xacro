<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="legrobot">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import legrobot urdf file -->
    <xacro:include filename="$(find lesson_urdf)/urdf/legrobot.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="legrobot.ros2_control.xacro" />


    <xacro:legrobot_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
